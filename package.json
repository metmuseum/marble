	"scripts": {
		"build": "webpack --config webpack.config.js --mode=production",
		"build-dev": "webpack --config webpack.config.js --mode=development",
		"build-storybook": "build-storybook -c .storybook -o out",
		"chromatic-ci": "chromatic --ci --exit-once-uploaded --storybook-build-dir out",
		"deploy-storybook": "storybook-to-ghpages",
		"lint": "eslint .",
		"start": "webpack-dev-server --open --hot --config webpack.dev.config.js",
		"storybook": "start-storybook -p 54525",
		"test:storybook": "pnpm  --package=concurrently --package=wait-on --package=http-server dlx concurrently -k -s first -n \"sb,test\" -c \"magenta,blue\" \"http-server out --port 6006 --silent\" \"wait-on http://127.0.0.1:6006 && node_modules/.bin/test-storybook --url http://127.0.0.1:6006\"",
		"test:storybook-ci": "pnpm  --package=concurrently --package=wait-on --package=http-server dlx concurrently -k -s first -n \"sb,test\" -c \"magenta,blue\" \"http-server out --port 6006 --silent\" \"wait-on http://127.0.0.1:6006 && node_modules/.bin/test-storybook --url http://127.0.0.1:6006\"",
		"stylelint": "stylelint '**/*.scss' --quiet",
		"postinstall": "node postinstall.js",
		"release": "release-it"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/metmuseum/marble.git"
